{
  "name": "dotan-landing",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist .astro node_modules pnpm-lock.yaml && find . -name \".DS_Store\" -delete",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\"",
    "format:diff": "prettier --list-different --cache \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\""
  },
  "dependencies": {
    "@astrojs/tailwind": "^5.1.3",
    "astro": "^5.0.1",
    "flowbite": "^2.5.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "prettier": "^3.4.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9"
  }
}