{
  "name": "dotan-landing",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist .astro node_modules pnpm-lock.yaml && find . -name \".DS_Store\" -delete",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\"",
    "format:diff": "prettier --list-different --cache \"src/**/*.{js,jsx,ts,tsx,astro,svelte,json,md,css}\"",
    "knip": "knip"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@emailjs/browser": "^4.4.1",
    "@fontsource-variable/heebo": "^5.2.8",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.8",
    "astro-icon": "^1.1.4",
    "astro-seo": "^0.8.4",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@iconify-json/ic": "^1.2.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "knip": "^5.80.2",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3"
  }
}
